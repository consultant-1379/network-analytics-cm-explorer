		<assert test='@initRto ge @minRto'><value-of select="."/> initRto must be &gt;= minRto</assert>
		<assert test='@initRto le @maxRto'><value-of select="."/> initRto must be &lt;= maxRto</assert>
		<assert test='@maxRto gt @minRto'><value-of select="."/> maxRto must be &gt; minRto</assert>
		<assert test='@maxActivateThr ge @minActivateThr'><value-of select="."/> maxActivateThr must be &gt;= minActivateThr</assert>
		<assert test='@primaryPathMaxRtx le @pathMaxRtx'><value-of select="."/> primaryPathMaxRtx must be &lt;= pathMaxRtx</assert>
		<assert test='@thrTransmitBuffer le @transmitBufferSize'><value-of select="."/> thrTransmitBuffer must be &lt;= transmitBufferSize</assert>
		</rule>
	</pattern>
</schema>]]></rule>
